extern fn printf(fmt: ...[i8]): i32;

struct Data { x: i32, y: f32 }

struct ComplexData { inner: Data }

fn main() {
    var myData: Data = Data { x: 100, y: 200.5 + 3 };
    const nestedData: ComplexData = ComplexData { inner: Data { x: 300, y: 100.2 - 5.5 } };
    return;
}

fn getData(): Data {
    return Data { x: 200, y: 3.5 };
